<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <ApplicationIcon>gpu.ico</ApplicationIcon>
    <IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
    <AssemblyName>GPU_Dword_Manager</AssemblyName>
    <Product>GPU Dword Manager</Product>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="AMD_EXPORT.txt" />
    <EmbeddedResource Include="Nvidia\0000 NVIDIA.txt">
      <LogicalName>AMD_DWORD_Viewer.NVIDIA_0000.txt</LogicalName>
    </EmbeddedResource>
    <EmbeddedResource Include="Nvidia\NVLDDMKM.txt">
      <LogicalName>AMD_DWORD_Viewer.NVIDIA_NVLDDMKM.txt</LogicalName>
    </EmbeddedResource>
    <EmbeddedResource Include="amd.ico">
      <LogicalName>AMD_DWORD_Viewer.amd.ico</LogicalName>
    </EmbeddedResource>
    <EmbeddedResource Include="nvidia.ico">
      <LogicalName>AMD_DWORD_Viewer.nvidia.ico</LogicalName>
    </EmbeddedResource>
  </ItemGroup>

</Project>
